params {
    kit                 = "SQK-RNA002"
    flowcell            = "FLO-MIN106"
    fast5               = "$baseDir/data2/**/*.fast5"
    reference           = "$baseDir/anno/curlcake_constructs_EcoRV_BamHI_digestion.fasta.gz"
    annotation          = "$baseDir/anno/curlcake_constructs_EcoRV_BamHI_digestion.gtf.gz"
    ref_type            = "genome"

    seq_type            = "cDNA"
    output              = "$baseDir/output"
    qualityqc           = 5
    granularity         = 2  

    basecalling         = "guppy"
    GPU                 = "ON"
    demultiplexing      = "NO"
//deeplexicon"
    demulti_fast5       = "NO" 

    filtering              = "nanofilt"
   
    mapping              = "graphmap2"

    counting             = "htseq"

    saveSpace           = "NO"

    email               = ""
}
