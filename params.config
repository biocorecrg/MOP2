params {
    kit                 = "SQK-RNA002"
    flowcell            = "FLO-MIN106"
    fast5               = "$baseDir/data2/**/*.fast5"
    reference           = "$baseDir/anno/curlcake_constructs_EcoRV_BamHI_digestion.fasta.gz"
    annotation          = ""
    ref_type            = "transcriptome"

    seq_type            = "cDNA"
    output              = "$baseDir/output2"
    qualityqc           = 5
    granularity         = 1  

    basecalling         = "guppy"
    GPU                 = "ON"
    demultiplexing      = "NO"
//guppy"
    demulti_fast5       = "ON" 

    filtering              = "nanofilt"
   
    mapping              = "minimap2"

    counting             = "htseq"

    variantcall         = "medaka"

    downsampling        = "NO"

    email               = ""
}
